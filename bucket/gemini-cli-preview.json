{
    "version": "0.8.0-preview.1",
    "description": "An open-source AI agent that brings the power of Gemini directly into your terminal (Preview)",
    "homepage": "https://google-gemini.github.io/gemini-cli/",
    "license": "Apache-2.0",
    "url": "https://registry.npmjs.org/@google/gemini-cli/-/gemini-cli-0.8.0-preview.1.tgz",
    "hash": "sha512:e36261162e8faeb41fb10d04146ef077de889b65bcd67350bda14e365985c74cfb293c242de93b6d44b3a0ee0371d4b3aff917b5e49b903ef928191c42565404",
    "extract_dir": "package",
    "depends": "main/nodejs-lts",
    "bin": [
        [
            "node",
            "gemini",
            "$dir\\dist\\index.js"
        ]
    ],
