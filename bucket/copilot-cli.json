{
    "version": "0.0.354",
    "description": "GitHub Copilot CLI brings the power of Copilot coding agent directly to your terminal",
    "homepage": "https://github.com/github/copilot-cli",
    "license": {
        "identifier": "Proprietary",
        "url": "https://docs.github.com/en/site-policy/github-terms/github-pre-release-license-terms"
    },
    "url": "https://registry.npmjs.org/@github/copilot/-/copilot-0.0.354.tgz",
    "hash": "sha512:be4ffcd0d2358e5812c8274d5817553cc0b467223c3c8180b304206b538bbbe04819001e23da24b35b69db739e5e291ad9c14c25256fdc625f4cc471fe0aa11c",
    "extract_dir": "package",
    "depends": "main/nodejs-lts",
    "bin": [
        [
            "node",
            "copilot",
            "$dir\\index.js"
        ]
    ],
    "post_install": [
        "Push-Location $dir",
        "npm install --omit=dev --silent",
        "Pop-Location"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@github/copilot/latest",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "url": "https://registry.npmjs.org/@github/copilot/-/copilot-$version.tgz",
        "hash": {
            "url": "https://registry.npmjs.org/@github/copilot/$version",
            "regex": "sha512-([A-Za-z0-9+/=]+)"
        }
    }
}
