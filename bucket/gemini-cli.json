{
    "version": "0.1.22",
    "description": "An open-source AI agent that brings the power of Gemini directly into your terminal",
    "homepage": "https://google-gemini.github.io/gemini-cli/",
    "license": "Apache-2.0",
    "url": "https://registry.npmjs.org/@google/gemini-cli/-/gemini-cli-0.1.22.tgz",
    "hash": "sha512:823c29bcaca40f10fd897314b2864cee36372df0a8e9ff38ea007d4fbed584369b7367691523e250dd68fe01787baebefcb03270aebabfe0c4af7e6a4b80c7bb",
    "extract_dir": "package",
    "depends": "main/nodejs-lts",
    "bin": [
        [
            "node",
            "gemini",
            "$dir/dist/index.js"
        ]
    ],
    "post_install": [
        "Push-Location \"$dir\"",
        "npm pack --ignore-scripts",
        "npm install --global --build-from-source --cache=\"$env:SCOOP_CACHE\\npm_cache\" --prefix=\"$dir\" \"*.tgz\"",
        "Pop-Location"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@google/gemini-cli/latest",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "url": "https://registry.npmjs.org/@google/gemini-cli/-/gemini-cli-$version.tgz",
        "hash": {
            "url": "https://registry.npmjs.org/@google/gemini-cli/$version",
            "regex": "sha512-([A-Za-z0-9+/=]+)"
        }
    }
}
