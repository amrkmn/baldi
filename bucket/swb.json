{
    "version": "0.1.0",
    "description": "Scoop With Bun (swb): A JavaScript implementation of Scoop using Bun's runtime",
    "homepage": "https://github.com/amrkmn/swb",
    "license": "MIT",
    "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-0.1.0.tgz",
    "hash": "sha512:6133c95e7d9dc2aa2c441dbd4ebef533a42504ea5363eb3ea3ea8b1b52662ad6c37b67fc1f6fb5b1c2c3ee78e9098dd1cb576fd5f1ef74d99af72fc081ab4c80",
    "extract_dir": "package",
    "depends": "main/bun",
    "bin": [
        [
            "bun",
            "swb",
            "$dir\\dist\\cli.js"
        ]
    ],
    "post_install": [
        "Push-Location $dir",
        "bun install --production --silent",
        "Pop-Location"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/latest",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-$version.tgz",
        "hash": {
            "url": "https://registry.npmjs.org/@amrkmn/swb/$version",
            "regex": "sha512-([A-Za-z0-9+/=]+)"
        }
    }
}
