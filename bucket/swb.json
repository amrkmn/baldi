{
    "version": "0.3.0",
    "description": "Scoop With Bun (swb): A JavaScript implementation of Scoop using Bun's runtime",
    "homepage": "https://github.com/amrkmn/swb",
    "license": "MIT",
    "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-0.3.0.tgz",
    "hash": "sha512:bef947c1d87da67c8b645899671da07c60a29c3bc193cc4bea24a9121e3c764cb62659bc964398f0ac7914b5524edb42f579f84122e889fc07da72874fa590b7",
    "extract_dir": "package",
    "depends": "main/bun",
    "bin": [
        [
            "bun",
            "swb",
            "$dir\\dist\\cli.js"
        ]
    ],
    "post_install": [
        "Push-Location $dir",
        "bun install --production --silent",
        "Pop-Location"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/latest",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-$version.tgz",
        "hash": {
            "url": "https://registry.npmjs.org/@amrkmn/swb/$version",
            "regex": "sha512-([A-Za-z0-9+/=]+)"
        }
    }
}
