{
    "version": "0.3.6",
    "description": "Scoop With Bun (swb): A JavaScript implementation of Scoop using Bun's runtime",
    "homepage": "https://github.com/amrkmn/swb",
    "license": "MIT",
    "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-0.3.6.tgz",
    "hash": "sha512:d1e2220a7b301b636d907ec641075754e45c8b361747c13c2c5419511dd2e7d8ba41cb41fd68c141b43955dc126d92ce025d910f538377ac776fe46c2e574f9f",
    "extract_dir": "package",
    "depends": "main/bun",
    "bin": [
        [
            "bun",
            "swb",
            "$dir\\dist\\cli.js"
        ]
    ],
    "post_install": [
        "Push-Location $dir",
        "bun install --production --silent",
        "Pop-Location"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/latest",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-$version.tgz",
        "hash": {
            "url": "https://registry.npmjs.org/@amrkmn/swb/$version",
            "regex": "sha512-([A-Za-z0-9+/=]+)"
        }
    }
}
