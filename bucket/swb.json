{
    "version": "0.5.2",
    "description": "A TypeScript reimplementation of Scoop using Bun's runtime",
    "homepage": "https://github.com/amrkmn/swb",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": [
                "https://github.com/amrkmn/swb/releases/download/v0.5.2/swb-v0.5.2-windows-x64.zip",
                "https://github.com/amrkmn/swb/releases/download/v0.5.2/swb-v0.5.2-windows-x64-baseline.zip"
            ],
            "hash": [
                "7f017c3d4f7915dff2b837d00285ff54347a1192eb9405571d0f9a945490f8e5",
                "07017e5ce8ecc35d01a173dcf8514f6ccd1ebfc7854cb9b89c30f0eafb0dd6b4"
            ]
        }
    },
    "pre_install": [
        "$hasAVX2 = Start-Job -ScriptBlock { Add-Type -MemberDefinition '[DllImport(\"kernel32.dll\")] public static extern bool IsProcessorFeaturePresent(int ProcessorFeature);' -Name Kernel32 -Namespace Win32; [Win32.Kernel32]::IsProcessorFeaturePresent(40) } | Receive-Job -Wait -AutoRemoveJob",
        "$folder = if (-not $hasAVX2) { \"swb-v$version-windows-x64-baseline\" } else { \"swb-v$version-windows-x64\" }",
        "Move-Item \"$dir\\$folder\\*\" \"$dir\" -Force",
        "Remove-Item \"$dir\\swb-v*\" -Recurse -Force"
    ],
    "bin": "swb.exe",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": [
                    "https://github.com/amrkmn/swb/releases/download/v$version/swb-v$version-windows-x64.zip",
                    "https://github.com/amrkmn/swb/releases/download/v$version/swb-v$version-windows-x64-baseline.zip"
                ]
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
