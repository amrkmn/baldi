{
    "version": "0.3.5",
    "description": "Scoop With Bun (swb): A JavaScript implementation of Scoop using Bun's runtime",
    "homepage": "https://github.com/amrkmn/swb",
    "license": "MIT",
    "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-0.3.5.tgz",
    "hash": "sha512:4342fad4c33a6156b65de7c440ab0e86e1111c64c51399f95cb5aa7abea4f344670db0b0a5438c2bb7de4c962d5a52888e046eb3d46253d0c7d7748d233527d8",
    "extract_dir": "package",
    "depends": "main/bun",
    "bin": [
        [
            "bun",
            "swb",
            "$dir\\dist\\cli.js"
        ]
    ],
    "post_install": [
        "Push-Location $dir",
        "bun install --production --silent",
        "Pop-Location"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/latest",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-$version.tgz",
        "hash": {
            "url": "https://registry.npmjs.org/@amrkmn/swb/$version",
            "regex": "sha512-([A-Za-z0-9+/=]+)"
        }
    }
}
