{
    "version": "0.3.7",
    "description": "Scoop With Bun (swb): A JavaScript implementation of Scoop using Bun's runtime",
    "homepage": "https://github.com/amrkmn/swb",
    "license": "MIT",
    "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-0.3.7.tgz",
    "hash": "sha512:426ce749edec67b252ca5cd2fb9e8bb10326b7f830d1fe7f8f4950ceb7ca9090a584f7378ceeaf8fbdea7b7ce0cfa18187f6cd2d0fb45a15391594ac1b45fd06",
    "extract_dir": "package",
    "depends": "main/bun",
    "bin": [
        [
            "bun",
            "swb",
            "$dir\\dist\\cli.js"
        ]
    ],
    "post_install": [
        "Push-Location $dir",
        "bun install --production --silent",
        "Pop-Location"
    ],
    "checkver": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/latest",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "url": "https://registry.npmjs.org/@amrkmn/swb/-/swb-$version.tgz",
        "hash": {
            "url": "https://registry.npmjs.org/@amrkmn/swb/$version",
            "regex": "sha512-([A-Za-z0-9+/=]+)"
        }
    }
}
